{
  "name": "@futoin/optihelp",
  "version": "1.2.1",
  "vcs": "git",
  "rms": "npm",
  "rmsRepo": "ignored",
  "toolTune": {
    "npm": {
      "access": "public"
    }
  },
  "plugins": {
    "release": "futoin.cid.misc.releasetool"
  },
  "actions": {
    "test": [
      "@cid tool envexec node -- sh -c \"test $NODE_ENV != production\"",
      "@cid tool exec grunt -- test"
    ],
    "upgrade-deps": [
      "@cid tool update",
      "@cid tool exec yarn -- upgrade --latest"
    ]
  }
}
